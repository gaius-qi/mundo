{
  "name": "webpack-vue-multiple-pages",
  "version": "1.0.0",
  "description": "Vue project multiple page configuration with Webpack4.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gaius",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "serve": "npm run dev:test &",
    "lint": "eslint --fix . --ext .vue,.jsx,.js && stylelint --fix src/**/*.less -s less",
    "tar": "tar czf dist.tgz dist && rm -rf dist",
    "build": " npm run build:clean && cross-env NODE_ENV=production webpack -p --mode production && npm run tar",
    "build:clean": "rm -rf dist",
    "build:report": "npm run build --report"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.53",
    "autoprefixer": "^9.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-istanbul": "^4.1.6",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^5.0.1",
    "eslint-config-antife": "^1.0.2",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-html": "^4.0.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.4.0",
    "stylelint-config-antife": "^1.0.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.3.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
